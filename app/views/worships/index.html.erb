<div class="contents row">
  <% @worships.each do |worship| %>
    <div class="content_post" style="background-image: url(<%= worship.image %>);">
    <div class="more">
        <span><%= image_tag 'arrow_top.png' %></span>
        <ul class="more_list">
          <li>
              <%= link_to '削除', worship_path(worship.id), method: :delete %>
          </li>
        </ul>
      </div>
      <p><%= worship.text %></p>
      <span class="name">
        <%= worship.name %>
      </span>
    </div>
  <% end %>
</div>